    },
    "tasks": {
      "tags": []
    },
    "tasks-e2e": {
      "tags": [],
      "implicitDependencies": ["tasks"]
    },
    "boards": {
      "tags": []
    },
    "boards-e2e": {
      "tags": [],
      "implicitDependencies": ["boards"]
    },
    "reports": {
      "tags": []
    },
    "reports-e2e": {
      "tags": [],
      "implicitDependencies": ["reports"]
